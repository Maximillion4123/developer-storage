basket = []
ball = []
N , M = map(int, input().split())
for x in range(1, N + 1):
    basket.append(0)
    ball.append(0)

for i in range(1, M + 1):
    a , b , c = map(int, input().split())
    for f in range(a , b + 1):
        basket[f-1] = c

for y in range(len(basket)):
    print(basket[y],end=' ')